package array;

import array.model.StudentVO;

public class StudentEx_01 {

	public static void main(String[] args) {
		
		StudentVO[] stVO = new StudentVO[3];
		for(int i = 0; i < 4 ; i++) {
			stVO[i] = new StudentVO();
		}
		
		stVO[0].strNum = "001";
		stVO[0].strName = "홍길동";
		stVO[0].intKor = 85;
		stVO[0].intEng = 72;
	}
}
