package array.model;

public class StudentVO {

	public String strNum;
	public String strName;
	public String strDeg;
	public int intKor;
	public int intEng;
	
	public int total;
	public float average;
	
	public void total() {
		total = intKor + intEng;
	}
	public void average() {
		average = (intKor + intEng) / 2;
	}

	
}
